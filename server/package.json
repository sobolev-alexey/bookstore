{
  "name": "bookstore",
  "version": "1.4.0",
  "description": "Bookstore backend application using NestJS, Prisma ORM and SQLite",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed": "prisma migrate dev --name init",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sobolev-alexey/bookstore.git"
  },
  "author": "Alexey Sobolev <hello@lexer.dev> (https://lexer.dev/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sobolev-alexey/bookstore/issues"
  },
  "homepage": "https://github.com/sobolev-alexey/bookstore#readme",
  "keywords": [],
  "dependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/mapped-types": "1.1.0",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/swagger": "^6.1.2",
    "@prisma/client": "^4.4.0",
    "axios": "^0.27.2",
    "csvtojson": "^2.0.10",
    "nanoid": "^4.0.0",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "stripe": "^10.12.0-beta.1",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.1.1",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "glob-parent": "^6.0.2",
    "node-forge": "^1.3.1",
    "nth-check": "^2.1.1",
    "terser": "^5.15.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
